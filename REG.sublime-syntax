%YAML 1.2
---
name: REG
scope: source.reg

file_extensions:
  - reg

contexts:
  main:
    - match: (?:REGEDIT4|Windows Registry Editor Version 5\.00)\b
      scope: meta.directive.registry.ini keyword.other.directive.registry.ini
      set: INI.sublime-syntax
    - match: ^|(?=\S)
      set: INI.sublime-syntax
